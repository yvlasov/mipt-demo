{ 
  "semantic_release": { 
    "remote": {
      "type": "github",
      "token": { "env": "GH_TOKEN" }
    },
    "tag_format": "v{version}",
    "changelog": {
      "default_templates": { "changelog_file": "CHANGELOG.md" }
    },
    "branches": {
      "main": {
        "match": "(main|master|add-semrelease)",
        "prerelease": "false"
      }
    }
  }
}
